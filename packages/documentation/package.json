{
  "name": "@aztec/documentation",
  "version": "0.0.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git"
  },
  "description": "AZTEC Protocol documentation based on React Styleguidist",
  "scripts": {
    "build": "truffle compile --all && styleguidist build && yarn copyApis && yarn copyImages",
    "clean": "shx rm -rf ./build  ./dist ./public || true",
    "copyApis": "cp -r src/apis public/apis",
    "copy:local": "find -E ../extension/src/client/apis -regex '.*(Zk|Account).*' -exec cp {} ./src/apis \\;",
    "copyImages": "cp -r ./images ./public",
    "generateStyles": "guacamole generateStyles",
    "lint": "eslint --ignore-path .eslintignore .",
    "start": "yarn styleguide",
    "styleguide": "styleguidist server",
    "test": "jest ./src --passWithNoTests",
    "transpile": "BABEL_ENV=transpile babel src -x \".js\",\".jsx\" --ignore \"**/*.spec.js\",\"**/demo/**\" -d dist"
  },
  "dependencies": {
    "@aztec/contract-addresses": "1.16.1",
    "@aztec/guacamole-ui": "^1.2.0",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.8.6",
    "@openzeppelin/contracts-ethereum-package": "^2.4.0",
    "@openzeppelin/gsn-provider": "^0.1.9",
    "acorn": "^7.1.0",
    "autoprefixer": "^9.5.0",
    "axios": "^0.19.0",
    "aztec.js": "^0.17.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "bn.js": "^5.1.1",
    "buble": "^0.19.8",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "clsx": "^1.1.0",
    "comment-parser": "^0.7.2",
    "console-feed": "^2.8.11",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "del": "^4.0.0",
    "dotenv": "^8.2.0",
    "estree-walker": "^1.0.1",
    "eth-crypto": "^1.5.1",
    "file-loader": "^3.0.1",
