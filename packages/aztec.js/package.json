    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "braintree-jsdoc-template": "^3.3.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
